<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchWrap.Vertical"
    android:background="@android:color/white"
    >
    <include layout="@layout/pos_common_wheel_title"/>
    <FrameLayout
        style="@style/MatchWrap"
        android:paddingLeft="30dp"
        android:paddingRight="30dp">

        <LinearLayout style="@style/MatchWrap.Horizontal">

            <wmpos.test.weight.WheelView
                android:id="@+id/wheel_month"
                style="@style/AutoWrap" />

            <wmpos.test.weight.WheelView
                android:id="@+id/wheel_day"
                style="@style/AutoWrap" />

            <wmpos.test.weight.WheelView
                android:id="@+id/wheel_hour"
                style="@style/AutoWrap" />

            <wmpos.test.weight.WheelView
                android:id="@+id/wheel_minuate"
                style="@style/AutoWrap" />
        </LinearLayout>

        <wmpos.test.weight.SpaceView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            app:SpaceViewType="ALL" />
    </FrameLayout>
</LinearLayout>
